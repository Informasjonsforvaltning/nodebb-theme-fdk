{
    "name": "nodebb-theme-fdk",
    "version": "0.1.0",
    "description": "Custom theming for the FDK community forum",
    "main": "theme.less",
    "keywords": [],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/Informasjonsforvaltning/nodebb-theme-fdk"
    },
    "bugs": {
        "url": "https://github.com/Informasjonsforvaltning/nodebb-theme-fdk/issues"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npx lint-staged",
            "commit-msg": "npx commitlint -E HUSKY_GIT_PARAMS"
        }
